{
  "project_id": "kisaan-mitra-prod",
  "credentials_path": "backend/agents/cmga/config/google-credentials-prod.json",
  "environment": "production",
  "vertex_ai": {
    "project_id": "kisaan-mitra-prod",
    "region": "us-central1",
    "model_endpoints": {
      "text-bison": "text-bison@001",
      "code-bison": "code-bison@001",
      "chat-bison": "chat-bison@001",
      "textembedding-gecko": "textembedding-gecko@001"
    }
  },
  "vision_api": {
    "enabled": true,
    "features": [
      "CROP_HINTS",
      "OBJECT_LOCALIZATION",
      "IMAGE_PROPERTIES",
      "SAFE_SEARCH_DETECTION"
    ],
    "max_results": 10
  },
  "natural_language": {
    "enabled": true,
    "features": [
      "EXTRACT_ENTITIES",
      "EXTRACT_DOCUMENT_SENTIMENT",
      "CLASSIFY_TEXT",
      "EXTRACT_SYNTAX"
    ],
    "language": "en"
  },
  "automl": {
    "enabled": true,
    "model_ids": {
      "crop_risk_assessment": "TBL_CROP_RISK_PROD",
      "yield_prediction": "TBL_YIELD_PRED_PROD",
      "price_forecasting": "TBL_PRICE_FORECAST_PROD",
      "fraud_detection": "TBL_FRAUD_DETECT_PROD"
    }
  },
  "rate_limits": {
    "requests_per_minute": 120,
    "requests_per_day": 10000,
    "concurrent_requests": 20,
    "retry_attempts": 5,
    "backoff_factor": 2.0,
    "max_backoff_time": 600
  },
  "security": {
    "encryption_enabled": true,
    "data_residency_region": "us",
    "audit_logging": true,
    "differential_privacy": true,
    "consent_required": true,
    "data_retention_days": 2555
